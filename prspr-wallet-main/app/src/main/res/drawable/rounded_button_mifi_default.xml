<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/mifi_custom_button_radius" />
            <stroke android:width="1dip" android:color="@color/mifi_blue_bb_theme_color" />
            <solid android:color="@color/mifi_blue_bb_theme_color"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/mifi_custom_button_radius" />
            <stroke android:width="1dip" android:color="@color/mifi_blue_bb_theme_color" />
            <solid android:color="@color/mifi_blue_bb_theme_color"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="@dimen/mifi_custom_button_radius" />
            <stroke android:width="1dip" android:color="@color/mifi_bar_start_color" />
            <solid android:color="@color/mifi_bar_start_color"/>
        </shape>
    </item>
</selector>